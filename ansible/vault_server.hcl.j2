backend "consul" {
  advertise_addr = "http://{{ hostvars[inventory_hostname]['ansible_eth0']['ipv4']['address'] }}:8200"
  address = "127.0.0.1:8500"
  path = "vault"
  service_tags = "urlprefix-vault.{{ tld }}/"
}

listener "tcp" {
  address = "{{ hostvars[inventory_hostname]['ansible_eth0']['ipv4']['address'] }}:8200"
  tls_disable = 1
}
